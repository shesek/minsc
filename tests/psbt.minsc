test("PSBT", |T| {
  // First test case in https://github.com/bitcoin/bips/blob/master/bip-0174.mediawiki#user-content-Test_Vectors
  test("BIP 174 Test Vector", |T| {
    $sk = tprv8ZgxMBicQKsPd9TeAdPADNnSyH9SSUUbTVeFszDE23Ki6TBB5nCefAdHkK8Fm3qMQR6sHwA56zqRmKmxnHk37JkiFzvncDqoKmPWubu7hDF;

    $psbt = psbt [
      "inputs": [
        75ddabb27b8845f5247975c8a5ba7c6f336c4570708ebe230caf6db5217ae858:0,
        1dea7cd05979072a3578cab271c02244ea8a090bbb46aa680a65ecd027048d83:1,
      ],
      "outputs": [
        script(0x0014d85c2b71d0060b09c9886aeb815e50991dda124d):1.4999 BTC,
        script(0x001400aea9a2e5f0f876a588df5546e8742d1d87008f):1 BTC,
      ],
    ];
    test("Create", |T| t::eq($psbt, psbt(0zcHNidP8BAJoCAAAAAljoeiG1ba8MI76OcHBFbDNvfLqlyHV5JPVFiHuyq911AAAAAAD/////g40EJ9DsZQpoqka7CwmK6kQiwHGyyng1Kgd5WdB86h0BAAAAAP////8CcKrwCAAAAAAWABTYXCtx0AYLCcmIauuBXlCZHdoSTQDh9QUAAAAAFgAUAK6pouXw+HaliN9VRuh0LR2HAI8AAAAAAAAAAAA)));
    log("Init PSBT:", pretty($psbt));

    $with_info = psbt::update($psbt, [
      "inputs": [
        0: [
          "non_witness_utxo": ,
          "redeem_script": 0x5221029583bf39ae0a609747ad199addd634fa6108559d6c5cd39b4c2183f1ab96e07f2102dab61ff49a14db6a7d02b0cd1fbb78fc4b18312b5b4e54dae4dba2fbfef536d752ae,
          "bip32_derivation": [
            $sk/0'/0'/0',
            $sk/0'/0'/1',
          ]
        ],
        1: [
          // "non_witness_utxo": ,
          "witness_utxo": `OP_HASH160 0xb7f5faf40e3d40a5a459b1db3535f2b72fa921e8 OP_EQUAL`:2 BTC,
          "witness_script": 0x522103089dc10c7ac6db54f91329af617333db388cead0c231f723379d1b99030b02dc21023add904f3d6dcf59ddb906b0dee23529b7ffb9ed50e5e86151926860221f0e7352ae,
          "redeem_script": 0x00208c2353173743b595dfb4a07b72ba8e42e3797da74e87fe7d9d7497e3b2028903,
          "bip32_derivation": [
            $sk/0'/0'/3',
            $sk/0'/0'/2',
          ]
        ],
      ],
      "outputs": [
        0: [ "bip32_derivation": [ $sk/0'/0'/4' ] ],
        1: [ "bip32_derivation": [ $sk/0'/0'/5' ] ],
      ],
    ]);
    test("Update", |T| t::eq($with_info, psbt()));

    $with_sighash = psbt::update($with_info, [
      "inputs": [
        0: [ "sighash_type": SIGHASH_ALL ],
        1: [ "sighash_type": SIGHASH_ALL ],
      ]
    ]);
    test("Update with sighash", |T| t::eq($with_sighash, psbt()));

    $sign1 = psbt::sign($with_sighash, [
      029583bf39ae0a609747ad199addd634fa6108559d6c5cd39b4c2183f1ab96e07f:cP53pDbR5WtAD8dYAW9hhTjuvvTVaEiQBdrz9XPrgLBeRFiyCbQr,
      03089dc10c7ac6db54f91329af617333db388cead0c231f723379d1b99030b02dc:cR6SXDoyfQrcp4piaiHE97Rsgta9mNhGTen9XeonVgwsh4iSgw6d
    ]);
    test("First signing", |T| t::eq($sign1, psbt()));

    $sign2 = psbt::sign($with_sighash, [
      02dab61ff49a14db6a7d02b0cd1fbb78fc4b18312b5b4e54dae4dba2fbfef536d7:cT7J9YpCwY3AVRFSjN6ukeEeWY6mhpbJPxRaDaP5QTdygQRxP9Au,
      023add904f3d6dcf59ddb906b0dee23529b7ffb9ed50e5e86151926860221f0e73:cNBc3SWUip9PPm1GjRoLEJT6T41iNzCYtD7qro84FMnM5zEqeJsE
    ]);
    test("Second signing", |T| t::eq($sign2, psbt()));

    $combined = psbt::combine[ $sign1, $sign2 ];
    test("Combine", |T| {
      t::eq($combined, psbt());
      t::eq($combined, $sign1 + $sign2);
      t::eq($combined, $sign2 + $sign1);
    });

    $final = psbt::finalize($combined);
    test("Finalize", |T| t::eq($final, psbt()));
    log("Final PSBT:", pretty($final));

    $tx = psbt::extract($final);
    test("Extract", |T| t::eq($tx, tx()));
    log("Final tx:", pretty($tx));
  })
})