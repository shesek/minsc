// Test vectors from https://github.com/bitcoin/bips/blob/master/bip-0119/vectors/ctvhash.json

test("CTV", |T| {
    test("Test Vector #1", |T| {
        // With scriptSigs
        $tx = tx();
        assert::eq(ctv::hash($tx, 0), 0x2d28d0672f1d46cb3e86abd7e682d2d3e9961e6c9237157f47d39f0a694bb694);
        assert::eq(ctv::hash($tx, 1), 0x12f7ab0a282fb9e29c9fd2ada21f950f492bfd5778a94202398c13ae6e97f0b4);
        assert::eq(ctv::hash($tx, 3116999548), 0x0ee9cc212182845d4c32ba6b3ba8859800d5cf423c58fb1444feaf21aa9cf81c);
        assert::eq(ctv::hash($tx, 4294967295), 0xda78ece7c0888725532355018961f58ad471f242e29a60adf84c55007fad608f);
        true
    });
    test("Test Vector #2", |T| {
        // No scriptSigs
        $tx = tx();
        assert::eq(ctv::hash($tx, 0), 0xe01a5d102bb5f8ba7986e7e4ab0fe8c4922bddd005adf740122684a91afad1a4);
        assert::eq(ctv::hash($tx, 1), 0x0980b070c9b5fea0a87b3b4a148876079cb54dc3db9ca560411c1c8cc3a7f5d9);
        assert::eq(ctv::hash($tx, 1311624179), 0x3871f92f02152b4846c1f00e3100be52b8ec53cff9f496061dcd3ad3f2f48a2a);
        assert::eq(ctv::hash($tx, 4294967295), 0x10b88c130695168a2fc2548e4a6f4fed11388a25f3da39a478ebbce6ac2c8299);
        true
    })
})